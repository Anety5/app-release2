<?xml version="1.0" encoding="utf-8"?>
<data-extraction-rules>
    <cloud-backup>
        <include domain="sharedpref" path="user_prefs.xml"/>
        <include domain="file" path="important_data/"/>
        <!-- No database/includes means DBs are excluded -->
    </cloud-backup>

    <device-transfer>
        <include domain="sharedpref" path="user_prefs.xml"/>
        <include domain="file" path="important_data/"/>
    </device-transfer>
</data-extraction-rules>


