# =========================
# Keystore configuration (example)
# =========================
# Replace with the actual absolute or relative path to your keystore (.jks)
MYAPP_UPLOAD_STORE_FILE=/path/to/your/some-fitness-release.jks

# Replace with your real store password
MYAPP_UPLOAD_STORE_PASSWORD=yourStorePasswordHere

# Replace with your key alias
MYAPP_UPLOAD_KEY_ALIAS=yourKeyAliasHere

# Replace with your key password
MYAPP_UPLOAD_KEY_PASSWORD=yourKeyPasswordHere

# =========================
# Gradle settings
# =========================
org.gradle.jvmargs=-Xmx2048m -Dfile.encoding=UTF-8
android.useAndroidX=true
android.enableJetifier=true
